{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "useBuiltIns": "usage",
        "corejs": 2
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ],
    "@babel/plugin-proposal-export-default-from",
    [	
      "module-resolver",	
      {	
        "alias": {	
          "@common": "./src/common",	
          "@components": "./src/components",	
          "@helpers": "./src/helpers",	
          "@utils": "./src/utils"	
        }	
      }	
    ],
    [
      "transform-inline-environment-variables",
      {
        "include": [
          "GG_EDITOR_VERSION"
        ]
      }
    ]
  ]
}
