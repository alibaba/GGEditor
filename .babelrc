{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "useBuiltIns": "usage",
        "corejs": "core-js@3",
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript",
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ],
    "@babel/plugin-proposal-export-default-from",
    [
      "transform-inline-environment-variables",
      {
        "include": [
          "GG_EDITOR_VERSION"
        ]
      }
    ]
  ]
}
